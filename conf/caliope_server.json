{
    "server": {
        "address": "0.0.0.0",
        "port": "9000",
        "debug": "True",
        "cache_enabled": "False",
        "static": "../caliope_webui_beowulf/app",
        "formTemplates": "templates/formTemplates"
    },
    /* Added fo app configurations */
    "app": {
        "languages": {
            "en": "English",
            "es": "Espa√±ol"
        },
        "modules": [
            {
                "file_uploader": {
                    "package": "core.file_uploader",
                    "route": "/upload"
                }
            },
            {
                "gis_proxy": {
                    "package": "modules.gis_proxy",
                    "route": "/gis_proxy"
                }
            },
            {
                "forms": {
                    "package": "core.forms",
                    "service": "form"
                }
            },
            {
                "login": {
                    "package": "core.login",
                    "service": "login"
                }
            },
            {
                "tasks": {
                    "package": "core.tasks",
                    "service": "tasks"
                }
            },
            {
                "projectsmtv": {
                    "package": "core.projects",
                    "service": "projects"
                }
            },
            {
                "accounts": {
                    "package": "core.accounts",
                    "service": "accounts"
                }
            },
            {
                "accounts": {
                    "package": "modules.reg2bogota",
                    "service": "catastro"
                }
            }
        ]
    },
    "storage": {
        "local": {
            "absolut_path": "./storage",
            "max_filesize": 10000000,
            "allowed_extensions": ["txt", "pdf", "png", "jpg", "jpeg", "gif", "odt", "pdf", "ods"]
        }
    }
}
