{
   "ng-controller":"LoginCtrl",
   "html":[
      {
         "type":"p",
         "html":"Login"
      },
      {
         "name":"login.id",
         "id":"txt-id",
         "caption":"Usuario",
         "type":"text",
         "ng-model":"login.username"
      },
      {
         "name":"login.password",
         "caption":"Password",
         "type":"password",
         "ng-model":"login.password"
      },
      {
         "type":"button",
         "ng-click":"authenticate(login)",
         "name":"login.login",
         "html":"Login"
      }
   ]
}